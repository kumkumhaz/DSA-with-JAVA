import util.java.*;

claas scannerClass{
    public public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String name = sc.next(); // will print only first word of the sentance given by the user in console. ex console :- kumkum kumari but output :- kumkum
        String name2 = sc.nextLine();  // will return whole input eg:- kumkum kumari , output:- kumkum kumari
        System.out.printlin(name);   
    }    
    
}